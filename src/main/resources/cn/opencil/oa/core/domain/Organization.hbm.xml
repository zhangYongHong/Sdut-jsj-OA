<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="cn.opencil.oa.core.domain.Organization" table="jsj_organization">
		<id name="id" type="java.lang.Long" length="5">
			<generator class="native"></generator>
		</id>
		<property name="name" type="java.lang.String" length="100"></property>
		<property name="parentId" type="java.lang.String" length="50" column="parent_id"></property>
		<property name="parentIds" type="java.lang.String" length="200" column="parent_ids"></property>
		<property name="available" type="java.lang.Boolean" length="1"></property>
	</class>
</hibernate-mapping>