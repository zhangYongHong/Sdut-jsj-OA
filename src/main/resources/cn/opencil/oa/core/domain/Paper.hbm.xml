<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.opencil.oa.core.domain.Paper" table="jsj_paper">
		<id name="uuid" type="java.lang.String"/>
		<property name="title" type="java.lang.String"/>
		<property name="author" type="java.lang.String"/>
		<property name="particpants" type="java.lang.String"/>
		<property name="level" type="java.lang.Integer"/>
		<property name="publishedDate" type="java.sql.Date"/>
		<property name="type" type="java.lang.Integer"/>
		<property name="volumePeriod" type="java.lang.String"/>
		<property name="annex" type="java.lang.String"/>
		<property name="schoolYear" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
