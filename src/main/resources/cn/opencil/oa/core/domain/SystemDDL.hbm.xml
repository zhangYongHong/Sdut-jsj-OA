<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.opencil.oa.core.domain.SystemDDL" table="jsj_systemddl">
		<id name="uuid" type="java.lang.String" length="50"/>
		<property name="keyword" type="java.lang.String"/>
		<property name="ddlCode" type="java.lang.Integer"/>
		<property name="ddlName" type="java.lang.String"/>
	</class>
</hibernate-mapping>
