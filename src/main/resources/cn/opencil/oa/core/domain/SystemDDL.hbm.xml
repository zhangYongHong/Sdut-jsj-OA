<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.opencil.oa.core.domain.SystemDDL" table="jsj_systemddl">
		<id name="sid" type="java.lang.Long" length="5">
			<generator class="native"></generator>
		</id>
		<property name="keyword" type="java.lang.String"></property>
		<property name="ddlCode" type="java.lang.Integer"></property>
		<property name="ddlName" type="java.lang.String"></property>
	</class>
</hibernate-mapping>
