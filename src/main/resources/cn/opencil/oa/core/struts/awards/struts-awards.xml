<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.ui.theme" value="simple"></constant>
	<constant name="struts.devMode" value="true"/>
	
	
	<package name="awards" extends="interceptor-login" namespace="/">
	
		<action name="awardsAction_*" class="awardsAction" method="{1}">
			<result name="listAction">/WEB-INF/jsp/paper/awards/list.jsp</result>
			<result name="addUI">/WEB-INF/jsp/paper/awards/add.jsp</result>
			<result name="redirect" type="redirectAction">
				<param name="actionName">awardsAction_list</param>
				<param name="namespace">/</param>
			</result>
			<result name="action2action" type="chain">
				<param name="actionName">awardsAction</param>
  				<param name="method">list</param>
			</result>
		</action>
		
	</package>
	
</struts>
	